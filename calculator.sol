  pragma solidity ^0.8.24;

    contract   Calculator {
     function add(int a, int b) public pure returns (int) {
         return (a+b);
         }

      function sub(int a,int b) public pure retuens{
         return(a-b);
       }


      function mul(int a, int b) public pure returns {
         return(a*b);
         }

     function div(int a, int b) public pure returns {
        return (a/b);
        }

      }

      
